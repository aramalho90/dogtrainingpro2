    <div class="w-25 p-3 float-right">
        <h4 class="display-5 text-center" style: "text-align: center">Vagas Grupo</h4>
        <table class="table table-striped table-dark table-hover table-sm mb-10">
        <thead>
            <tr>
                <th>Hora</th>
                <th>Em treino</th>
                <th>Limite</th>
            </tr>
        </thead>

        <tbody>
            <% @vacanciesG.each do |v| %>
                <tr>
                    <td><%= v.time %></td>
                    <% if v.count < v.limit %>
                    <td class="green"><%= v.count %></td>
                    <% elsif v.count == v.limit %>
                    <td class="yellow"><%= v.count %></td>
                    <% else %>
                    <td class="red"><%= v.count %></td>
                    <% end %>
                    <td><%= number_with_precision(v.limit).to_i  %></td>
                </tr>
            <% end %>
        </tbody>
        </table>
    </div>

    <div class="w-25 p-3 float-right">
        <h4 class="display-5 text-center" style: "text-align: center">Vagas PT</h4>
